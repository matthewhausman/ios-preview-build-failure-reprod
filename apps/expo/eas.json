{
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "ios": {
        "simulator": true,
        "image": "latest",
        "resourceClass": "m-medium"
      },
      "env": {
        "TAMAGUI_TARGET": "native"
      }
    },
    "preview": {
      "channel": "preview",
      "distribution": "internal",
      "env": {
        "TAMAGUI_TARGET": "native"
      },
      "ios": {
        "resourceClass": "m-medium"
      }
    },
    "production": {
      "distribution": "store",
      "android": {
        "buildType": "app-bundle"
      },
      "env": {
        "TAMAGUI_TARGET": "native"
      },
      "ios": {
        "resourceClass": "m-medium"
      }
    }
  }
}
